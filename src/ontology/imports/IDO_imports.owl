                                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000015"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0002297"/>
